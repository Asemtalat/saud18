<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- =======================
         Fleet Brands
    ======================= -->
    <record id="fleet_brand_opel" model="fleet.vehicle.model.brand">
        <field name="name">Opel</field>
    </record>

    <record id="fleet_brand_audi" model="fleet.vehicle.model.brand">
        <field name="name">Audi</field>
    </record>

    <record id="fleet_brand_bmw" model="fleet.vehicle.model.brand">
        <field name="name">BMW</field>
    </record>

    <record id="fleet_brand_mercedes" model="fleet.vehicle.model.brand">
        <field name="name">Mercedes</field>
    </record>

    <!-- =======================
         Vehicle Models
    ======================= -->
    <record id="fleet_model_opel_astra" model="fleet.vehicle.model">
        <field name="name">Astra</field>
        <field name="brand_id" ref="fleet_brand_opel"/>
    </record>

    <record id="fleet_model_audi_a1" model="fleet.vehicle.model">
        <field name="name">A1</field>
        <field name="brand_id" ref="fleet_brand_audi"/>
    </record>

    <record id="fleet_model_bmw_serie1" model="fleet.vehicle.model">
        <field name="name">Serie 1</field>
        <field name="brand_id" ref="fleet_brand_bmw"/>
    </record>

    <record id="fleet_model_mercedes_a" model="fleet.vehicle.model">
        <field name="name">Class A</field>
        <field name="brand_id" ref="fleet_brand_mercedes"/>
    </record>

    <record id="fleet_model_opel_corsa" model="fleet.vehicle.model">
        <field name="name">Corsa</field>
        <field name="brand_id" ref="fleet_brand_opel"/>
    </record>

    <!-- =======================
         Product Records
    ======================= -->
    <record id="product_product_fleet_1" model="product.product">
        <field name="name">Opel/Astra/1-ACK-205</field>
        <field name="standard_price">800.0</field>
        <field name="list_price">320.0</field>
        <field name="type">consu</field>
        <field name="weight">0.01</field>
        <field name="uom_id" ref="uom.product_uom_unit"/>
        <field name="uom_po_id" ref="uom.product_uom_unit"/>
    </record>

    <record id="product_product_fleet_2" model="product.product">
        <field name="name">Audi/A1/1-AUD-001</field>
        <field name="standard_price">800.0</field>
        <field name="list_price">320.0</field>
        <field name="type">consu</field>
        <field name="weight">0.01</field>
        <field name="uom_id" ref="uom.product_uom_unit"/>
        <field name="uom_po_id" ref="uom.product_uom_unit"/>
    </record>

    <record id="product_product_fleet_3" model="product.product">
        <field name="name">BMW/Serie 1/1-BMW-001</field>
        <field name="standard_price">800.0</field>
        <field name="list_price">320.0</field>
        <field name="type">consu</field>
        <field name="weight">0.01</field>
        <field name="uom_id" ref="uom.product_uom_unit"/>
        <field name="uom_po_id" ref="uom.product_uom_unit"/>
    </record>

    <record id="product_product_fleet_4" model="product.product">
        <field name="name">Mercedes/Class A/1-MER-001</field>
        <field name="standard_price">800.0</field>
        <field name="list_price">320.0</field>
        <field name="type">consu</field>
        <field name="weight">0.01</field>
        <field name="uom_id" ref="uom.product_uom_unit"/>
        <field name="uom_po_id" ref="uom.product_uom_unit"/>
    </record>

    <record id="product_product_fleet_5" model="product.product">
        <field name="name">Opel/Corsa/1-SYN-404</field>
        <field name="standard_price">800.0</field>
        <field name="list_price">320.0</field>
        <field name="type">consu</field>
        <field name="weight">0.01</field>
        <field name="uom_id" ref="uom.product_uom_unit"/>
        <field name="uom_po_id" ref="uom.product_uom_unit"/>
    </record>

    <!-- =======================
         Fleet Vehicle Records
    ======================= -->
    <record id="fleet_vehicle_1" model="fleet.vehicle">
        <field name="model_id" ref="fleet_model_opel_astra"/>
        <field name="license_plate">1-ACK-205</field>
        <field name="odometer_unit">kilometers</field>
        <field name="product_id" ref="product_product_fleet_1"/>
    </record>

    <record id="fleet_vehicle_2" model="fleet.vehicle">
        <field name="model_id" ref="fleet_model_audi_a1"/>
        <field name="license_plate">1-AUD-001</field>
        <field name="odometer_unit">kilometers</field>
        <field name="product_id" ref="product_product_fleet_2"/>
    </record>

    <record id="fleet_vehicle_3" model="fleet.vehicle">
        <field name="model_id" ref="fleet_model_bmw_serie1"/>
        <field name="license_plate">1-BMW-001</field>
        <field name="odometer_unit">kilometers</field>
        <field name="product_id" ref="product_product_fleet_3"/>
    </record>

    <record id="fleet_vehicle_4" model="fleet.vehicle">
        <field name="model_id" ref="fleet_model_mercedes_a"/>
        <field name="license_plate">1-MER-001</field>
        <field name="odometer_unit">kilometers</field>
        <field name="product_id" ref="product_product_fleet_4"/>
    </record>

    <record id="fleet_vehicle_5" model="fleet.vehicle">
        <field name="model_id" ref="fleet_model_opel_corsa"/>
        <field name="license_plate">1-SYN-404</field>
        <field name="odometer_unit">kilometers</field>
        <field name="product_id" ref="product_product_fleet_5"/>
    </record>
</odoo>
